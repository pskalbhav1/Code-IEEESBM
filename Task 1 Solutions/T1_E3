#include<iostream>
#include<stdio.h>
using namespace std;
int main()
{
    float a,b,sum,diff,multi,div;
    cout<<"enter nums";
    cin>>a>>b;
    sum=a+b;
    cout<<"sum is \n"<<sum;
    diff=a-b;
    cout<<"diff is \n"<<diff;
    multi=a*b;
    cout<<"product is \n"<<multi;
    div=a/b;
    cout<<"div is \n"<<div;
    return 0;
}
